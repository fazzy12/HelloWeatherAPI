{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/server.js"
      }
    ],
    "env": {
      "IPINFO_TOKEN": "@ipinfo_token",
      "OPENWEATHERMAP_API_KEY": "@openweathermap_api_key"
    }
  }
